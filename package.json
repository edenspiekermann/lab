{
  "name": "lab-website-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run serve",
    "watch": "npm run clean-copy && npm run webpack-watch & npm run stylus-watch & npm run nodemon",
    "---": "",
    "build": "npm run clean-copy && npm run webpack-build && npm run stylus-build && npm run autoprefixer",
    "stylus-build": "stylus ./app/index.styl -o dist -c",
    "webpack-build": "webpack -p",
    "autoprefixer": "autoprefixer ./dist/index.css -o ./dist/index.css",
    "webpack-watch": "webpack --watch -d --sourcemap",
    "stylus-watch": "stylus ./app/index.styl -o dist -u autoprefixer-stylus --watch",
    "serve": "node app/server.js",
    "nodemon": "nodemon --debug app/server.js",
    "clean-copy": "npm run clean && npm run copy-assets",
    "copy-assets": "cp -R app/assets/ dist/",
    "clean": "rm -Rf dist/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edenspiekermann/lab-website-web.git"
  },
  "author": "edenspiekermann",
  "bugs": {
    "url": "https://github.com/edenspiekermann/lab-website-web/issues"
  },
  "homepage": "https://github.com/edenspiekermann/lab-website-web",
  "dependencies": {
    "autoprefixer": "^5.0.0",
    "axios": "^0.5.0",
    "body-parser": "^1.10.2",
    "compression": "^1.3.0",
    "cookie-session": "^1.1.0",
    "express": "^4.11.1",
    "json-loader": "^0.5.1",
    "jsx-loader": "^0.12.2",
    "mout": "^0.11.0",
    "node-jsx": "^0.12.4",
    "react": "^0.12.2",
    "react-router": "^0.11.6",
    "serve-favicon": "^2.2.0",
    "stylus": "^0.49.3",
    "webpack": "^1.5.3",
    "when": "^3.6.4"
  },
  "devDependencies": {
    "autoprefixer-stylus": "^0.5.0",
    "nodemon": "^1.3.2"
  }
}
